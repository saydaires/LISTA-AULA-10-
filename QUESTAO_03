programa{
	funcao inicio(){
		inteiro i, n, soma = 0

		para(i = 1; i <= 5; i++){
			escreva("Forneça o número ", i, ": ")
			leia(n)

			se(n < 0){
                    // A expressão "soma++" indica que sempre que n < 0, será acrescido +1 no valor de soma.
				soma++
			}
		}
			escreva("Você forneceu ", soma, " números negativos.")
		}
	}

